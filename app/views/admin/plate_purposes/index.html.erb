
<%= page_title 'Plate Purposes', 'List' %>
<p>This is equivalent to the plate types in SNP.</p>
<table class="table">
  <tr>
    <th>Name</th>
    <th></th>
  </tr>

<% for plate_purpose in @plate_purposes %>
  <tr>
    <td><%=h plate_purpose.name %></td>
    <td><%= link_to 'Edit', [:edit,:admin,plate_purpose], title: "Edit #{plate_purpose.name}" %></td>
  </tr>
<% end %>
</table>

