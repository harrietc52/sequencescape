
<% last_awaiting_sub =  @awaiting_submissions.last %>
<% if last_awaiting_sub.present? %>
  <%= case last_awaiting_sub.state
  when "pending" then  display_user_guide(I18n.t('studies.submissions.pending_submission_waiting'), last_awaiting_sub)
  when "failed" then display_user_error(I18n.t('studies.submissions.last_submission_failed'), last_awaiting_sub)
  end
  %>
<% end  %>
