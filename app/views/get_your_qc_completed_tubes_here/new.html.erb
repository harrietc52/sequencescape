<div class="page-header"><h1>Get Your QC Completed Tubes Here</h1></div>

<%= form_tag(get_your_qc_completed_tubes_here_path) do -%>
  <fieldset>
    <div class="form-group">
      <%= label_tag :barcode %>
      <%= text_field_tag :barcode %>
    </div>
    <div class="form-group">
      <%= submit_tag 'Create tubes' %>
    </div>
  </fieldset>
<% end -%>
